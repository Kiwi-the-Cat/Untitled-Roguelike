[editor_plugins]

enabled=PackedStringArray("res://addons/QuickPluginManager/plugin.cfg", "res://addons/better-terrain/plugin.cfg", "res://addons/cheatsheet/plugin.cfg", "res://addons/godotsize/plugin.cfg")

